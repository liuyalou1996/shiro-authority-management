<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.universe.mapper.OperationLogMapper">
	<insert id="saveOperationLog" parameterType="operationlogdo">
		insert into tbl_operation_log(op_type,op_content,user_id,create_time)
		values(#{opType},#{opContent},#{userId},#{createTime})
	</insert>
</mapper>